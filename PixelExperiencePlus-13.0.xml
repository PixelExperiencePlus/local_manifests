<?xml version="1.0" encoding="UTF-8"?>
<manifest>

<!-- PixelExperiencePlus remote start-->
  <remote name="PE-Plus"
          fetch="https://github.com/PixelExperiencePlus"
          revision="thirteen-plus-wip" />
<!-- PixelExperiencePlus remote end-->

<!-- PixelExperiencePlus remove start-->
  <remove-project name="build" />
  <remove-project name="device_custom_sepolicy" />
  <remove-project name="device_qcom_sepolicy" />
  <remove-project name="device_qcom_sepolicy-legacy-um" />
  <remove-project name="device_qcom_sepolicy_vndr" />
