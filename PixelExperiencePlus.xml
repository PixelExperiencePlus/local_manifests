<?xml version="1.0" encoding="UTF-8"?>
<manifest>

<!-- PixelExperiencePlus remote start-->
  <remote name="PE-Plus"
          fetch="https://github.com/PixelExperiencePlus"
          revision="thirteen-plus-wip" />
<!-- PixelExperiencePlus remote end-->

<!-- PixelExperiencePlus remove start-->
  <remove-project name="build" />
  <remove-project name="frameworks_av" />
  <remove-project name="frameworks_base" />
  <remove-project name="packages_apps_CarrierConfig" />
  <remove-project name="packages_apps_Settings" />
  <remove-project name="packages_apps_TouchGestures" />
  <remove-project name="packages_apps_Updates" />
  <remove-project name="packages_overlays_ThemeIcons" />
  <remove-project name="packages_services_Telephony" />
  <remove-project name="system_update_engine" />
  <remove-project name="vendor_aosp" />
  <remove-project name="vendor_pixel-framework" />
<!-- PixelExperiencePlus remove end-->


<!-- PixelExperiencePlus add start-->

  <project path="build/make" name="PixelExperience_build" remote="PE-Plus">
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>

  <project path="frameworks/av"
	   name="PixelExperience_frameworks_av"
	   remote="PE-Plus" />

  <project path="frameworks/base"
	   name="PixelExperience_frameworks_base"
	   remote="PE-Plus" />

  <project path="packages/apps/CarrierConfig"
	   name="PixelExperience_packages_apps_CarrierConfig"
	   remote="PE-Plus" />

  <project path="packages/apps/GameSpace"
	   name="PixelExperience_packages_apps_GameSpace"
	   remote="PE-Plus" />

  <project path="packages/apps/Settings"
	   name="PixelExperience_packages_apps_Settings"
	   remote="PE-Plus" />

  <project path="packages/apps/TouchGestures"
	   name="PixelExperience_packages_apps_TouchGestures"
	   remote="PE-Plus" />

  <project path="packages/apps/Updates"
	   name="PixelExperience_packages_apps_Updates"
	   remote="PE-Plus" />

  <project path="packages/overlays/ThemeIcons"
	   name="PixelExperience_packages_overlays_ThemeIcons"
	   remote="PE-Plus" />

  <project path="packages/services/Telephony"
	   name="PixelExperience_packages_services_Telephony"
	   remote="PE-Plus" />

  <project path="system/update_engine"
	   name="PixelExperience_system_update_engine"
	   remote="PE-Plus" />

  <project path="vendor/aosp"
	   name="PixelExperience_vendor_aosp"
	   remote="PE-Plus" />

  <project path="vendor/pixel-framework"
	   name="PixelExperience_vendor_pixel-framework"
	   remote="PE-Plus" />
<!-- PixelExperiencePlus add end-->

</manifest>












